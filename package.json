{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "migrate:dev": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex migrate:latest --env development --cwd ./server",
    "migrate:prod": "npx knex migrate:latest --env production --cwd ./server",
    "rollback": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex migrate:rollback --cwd ./server",
    "rollback:all": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex migrate:rollback --all --cwd ./server",
    "migrate:list": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex migrate:list --cwd ./server",
    "lint": "eslint . --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.6.7",
