{
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "migrate:dev": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex migrate:latest --env development --cwd ./server",
    "migrate:prod": "npx knex migrate:latest --env production --cwd ./server",
    "rollback": "npx knex migrate:rollback --cwd ./server",
    "rollback:all": "npx knex migrate:rollback --all --cwd ./server",
    "migrate:list": "npx knex migrate:list --cwd ./server",
    "lint": "eslint . --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.6.5",
    "@heroicons/vue": "^1.0.6",
    "@nuxtjs/color-mode": "^3.0.3",
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^5.1.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^18.0.0",
    "@vueuse/nuxt": "^8.7.5",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nuxt": "3.0.0-rc.4",
    "nuxt-lodash": "^2.2.0",
    "postcss": "^8.4.13",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge",
    "@pinia/nuxt": "^0.1.8",
    "@vueuse/motion": "^2.0.0-beta.18",
    "animate.css": "^4.1.1",
    "argon2": "^0.28.5",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "pinia": "^2.0.14"
  }
}
