    "migrate:list:dev": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' npx knex migrate:list --env development --cwd ./server",
    "lint": "eslint . --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier . --write"
